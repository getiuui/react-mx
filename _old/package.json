{
  "name": "react-mx",
  "version": "0.1.0",
  "license": "MIT",
  "description": "create react components visually",
  "repository": "https://github.com/getiuui/react-mx.git",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "reset-dev": "scripts/reset-dev.sh",
    "init-dev": "scripts/prepare-dev-links.sh",
    "dev": "npm run reset-dev && concurrently \"npm run start\" \"npm run examples:playground:dev\"",
    "examples:playground:dev": "cd ./examples/playground && yarn dev",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {
    "react": ">=16.0",
    "react-dom": ">=16.0",
    "webpack": ">=4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "author": "https://iuui.io",
  "module": "dist/react-mx.esm.js",
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@rollup/plugin-image": "^2.0.5",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.22",
    "concurrently": "^5.3.0",
    "husky": "^4.2.5",
    "rollup-plugin-ignore": "^1.0.9",
    "rollup-plugin-less": "^1.1.2",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/layout": "^1.0.0-next.3",
    "@chakra-ui/system": "^1.0.0-next.3",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "@types/react-panelgroup": "^1.0.1",
    "antd": "^4.6.4",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "js-convert-case": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-docgen": "^5.3.0",
    "react-docgen-typescript": "^1.20.4",
    "react-dom": "^16.13.1",
    "react-panelgroup": "^1.0.12",
    "tiny-glob": "^0.2.6"
  }
}
