    "width": {
      "type": "input",
      "valueType": "number",
      "default": 100,
      "label": "Width",
      "controlProps": {
        "type": "text"
      }
    },
    "height": {
      "type": "input",
      "valueType": "number",
      "default": 30,
      "controlProps": {
        "type": "text"
      }
    }
  }
}
